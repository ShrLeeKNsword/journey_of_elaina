{"id":2,"name":"EV002","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":101,"indent":0,"parameters":["SF_Actor1",4,0,2,""]},{"code":401,"indent":0,"parameters":["阿贝尔"]},{"code":401,"indent":0,"parameters":["……米娜？"]},{"code":101,"indent":0,"parameters":["Elaina",0,0,2,""]},{"code":401,"indent":0,"parameters":["伊蕾娜"]},{"code":401,"indent":0,"parameters":["是，就是我喔。你还好吗？"]},{"code":101,"indent":0,"parameters":["SF_Actor1",4,0,2,""]},{"code":401,"indent":0,"parameters":["阿贝尔"]},{"code":401,"indent":0,"parameters":["你回来了……我还以为你不回来了。"]},{"code":101,"indent":0,"parameters":["Elaina",0,0,2,""]},{"code":401,"indent":0,"parameters":["伊蕾娜"]},{"code":401,"indent":0,"parameters":["我是你的恋人喔，不管发生什么都一定会回来呀。"]},{"code":101,"indent":0,"parameters":["SF_Actor1",4,0,2,""]},{"code":401,"indent":0,"parameters":["阿贝尔"]},{"code":401,"indent":0,"parameters":["……也是。"]},{"code":101,"indent":0,"parameters":["Elaina",0,0,2,""]},{"code":401,"indent":0,"parameters":["伊蕾娜"]},{"code":401,"indent":0,"parameters":["我找到百病药了喔。"]},{"code":101,"indent":0,"parameters":["Elaina",0,0,2,""]},{"code":401,"indent":0,"parameters":["伊蕾娜"]},{"code":401,"indent":0,"parameters":["我会跟今天的晚饭一起带来。你要多吃一点，再吞一颗"]},{"code":401,"indent":0,"parameters":["下去。说不定会花一点时间，可是你一定会好起来的。"]},{"code":101,"indent":0,"parameters":["SF_Actor1",4,0,2,""]},{"code":401,"indent":0,"parameters":["阿贝尔"]},{"code":401,"indent":0,"parameters":["我说啊，米娜。你能握住我的手吗？"]},{"code":101,"indent":0,"parameters":["",0,1,1,""]},{"code":401,"indent":0,"parameters":["他用迟钝的动作从被窝中伸出手。那并非年轻男性应有富有肌肉的手，"]},{"code":401,"indent":0,"parameters":["而是双皮包骨的枯枝。"]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":[""]},{